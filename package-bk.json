{
  "name": "single-spa-portal-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-all": "npm run install:config && npm run install:navbar && npm run install:dog && npm run install:cat",
    "install:config": "npm i",
    "install:navbar": "cd ./navbar/ && npm i",
    "install:dog": "cd ./dog/ && npm i",
    "install:cat": "cd ./cat/ && npm i",
    "start": "concurrently --kill-others --kill-others-on-fail -p name --names \"config,common-deps,navbar,dog,cat\" \"npm run start:config\" \"npm run start:common-deps\" \"npm run start:navbar\" \"npm run start:dog\" \"npm run start:cat\"",
    "start:config": "webpack-dev-server --config ./cfg/webpack.config.dev.js --port 8233",
    "start:common-deps": "webpack-dev-server --config ./cfg/webpack.common-deps.dev.js --port 8234",
    "start:navbar": "cd ./navbar/ && webpack-dev-server --config ./cfg/webpack.base.spa.js --port 8235",
    "start:dog": "cd ./dog/ && webpack-dev-server --config ./cfg/webpack.base.spa.js --port 8236",
    "start:cat": "cd ./cat/ && webpack-dev-server --config ./cfg/webpack.base.spa.js --port 8237",
    "build": "npm run build:config && npm run build:common-deps && npm run build:navbar && npm run build:dog && npm run build:cat",
    "build:config": "webpack --env.production --config ./cfg/webpack.config.js",
    "build:common-deps": "webpack --env.production --config ./cfg/webpack.common-deps.js",
    "build:navbar": "cd ./navbar/ && webpack --env.production --config ./cfg/webpack.base.spa.js",
    "build:dog": "cd ./dog/ && webpack --env.production --config ./cfg/webpack.base.spa.js",
    "build:cat": "cd ./cat/ && webpack --env.production --config ./cfg/webpack.base.spa.js",
    "audit-fix:config": "npm audit fix",
    "audit-fix:navbar": "cd ./navbar/ && npm audit fix",
    "audit-fix:dog": "cd ./dog/ && npm audit fix",
    "audit-fix:cat": "cd ./cat/ && npm audit fix",
    "postinstall": "node postinstall.js"
  },
  "author": "Justin McMurdie",
  "license": "ISC",
